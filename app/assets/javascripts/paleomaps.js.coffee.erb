window.Paleomaps =
  Models: {}
  Collections: {}
  Views: {}
  Routers: {}
  init: ->
  	new Paleomaps.Routers.Places()
  	Backbone.history.start()

  initMap: ->
  	mapDiv = $('#map')
  	mapOptions =
	  	mapTypeId: google.maps.MapTypeId.ROADMAP
  		center: new google.maps.LatLng(mapDiv.data('latitude'), mapDiv.data('longitude'))
  		zoom: 12
  	mapOptions
  	map = new google.maps.Map(mapDiv[0], mapOptions)

$(document).ready ->
  Paleomaps.init()

$(window).load ->
  Paleomaps.initMap()